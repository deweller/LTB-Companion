{
   "author": "Joe Looney",
   "background": {
      "scripts": [ "jquery.min.js", "openpgp.min.js", "bitcoinjs-min.js", "crypto.js", "tx.js", "util.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon-128.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Ctrl+Shift+K"
         }
      }
   },
   "description": "In Development - Forked from KryptoKit",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   },
   
   "manifest_version": 2,
   "minimum_chrome_version": "16.0.884",
   "name": "LTB Companion",
   "permissions": [ "clipboardWrite", "storage", "\u003Call_urls\u003E" ],
   "short_name": "Tokenly Tools",
   "version": "0.0.1"
}
